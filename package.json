{
  "name": "super-sario",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/eric-dahlgren-nti-johanneberg/super-sario.git",
  "author": "eric <eric@dgren.dev>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider poi src/index.html --serve",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider poi src/index.html --prod",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext js,ts",
    "format": "prettier --write \"@(src|public)/**/*.@(ts|js|json|html)\"",
    "ci": "npm-run-all --parallel --print-label lint typecheck build"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "npm-run-all": "4.1.5",
    "poi": "12.10.3",
    "prettier": "2.5.1",
    "typescript": "^4.6.0"
  }
}
